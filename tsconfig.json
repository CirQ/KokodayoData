{
  "compilerOptions": {
    "target": "es2016",
    "module": "esnext",
    "esModuleInterop": true,
    "incremental": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "vite/client",
    ],
    "typeRoots": [
      "./typings"
    ],
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}