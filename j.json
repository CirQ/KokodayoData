{
  "compilerOptions": {
    "target": "es2016",
    "esModuleInterop": true,
    "incremental": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "vite/client",
    ],
    "typeRoots": [
      "./typings"
    ],
    "paths": {
      "/~/*": [
        "src/*"
      ],
    }
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}