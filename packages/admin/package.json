{
  "name": "admin",
  "private": true,
  "engines": {
    "node": ">=v14.16.0",
    "npm": ">=7.6.3"
  },
  "version": "1.0.0",
  "main": "dist/source/main/index.cjs.js",
  "scripts": {
    "buildEnvTypes": "node ./bin/buildEnvTypes.js",
    "build": "node ./bin/build.js",
    "precompile": "windicss src/renderer /**/*.vue -to ./src/renderer/windi.css  && cross-env MODE=production yarn build ",
    "compile": "electron-builder build --config ./config/electron-builder.js",
    "pretest": "cross-env MODE=test yarn build",
    "test": "node ./tests/app.spec.js",
    "watch": "node ./bin/watch.js",
    "lint": "eslint . --ext js,ts,vue",
    "pretypecheck": "yarn buildEnvTypes",
    "typecheck-main": "tsc --noEmit -p ./src/main/tsconfig.json",
    "typecheck-preload": "tsc --noEmit -p ./src/preload/tsconfig.json",
    "typecheck-renderer": "vuedx-typecheck ./src/renderer --no-pretty",
    "typecheck": "yarn typecheck-main && yarn typecheck-preload && yarn typecheck-renderer",
    "unpack": "npx asar extract dist/app/win-unpacked/resources/app.asar dist/app/asar-unpacked/ "
  },
  "browserslist": [
    "Chrome 89"
  ],
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.7",
    "@vuedx/typecheck": "^0.6.3",
    "chokidar": "^3.5.1",
    "cross-env": "^7.0.3",
    "electron": "^12.0.1",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.22.0",
    "eslint-plugin-vue": "^7.7.0",
    "spectron": "^14.0.0",
    "typescript": "^4.2.3",
    "vite": "^2.0.5",
    "vite-plugin-windicss": "^0.8.2",
    "windicss": "^2.4.3"
  },
  "dependencies": {
    "@kkdy/api": "1.0.0",
    "@kkdy/data": "1.0.0",
    "electron-updater": "^4.3.8",
    "element-plus": "1.0.2-beta.33",
    "vue": "^3.0.7",
    "vue-router": "^4.0.5"
  }
}
